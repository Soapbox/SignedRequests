{
    "name": "soapbox/signed-requests",
    "description": "A wrapper to add the ability to accept signed requests to a Laravel project.",
    "keywords": ["laravel", "signed", "requests", "signed-requests"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jaspaul Bola",
            "email": "jaspaul.b@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Soapbox/fake-requests"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "illuminate/http": "^9",
        "illuminate/support": "^9",
        "ramsey/uuid": "^4.7.6"
    },
    "require-dev": {
        "jshayes/fake-requests": "dev-php81",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^7.42.0",
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\SignedRequests\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
